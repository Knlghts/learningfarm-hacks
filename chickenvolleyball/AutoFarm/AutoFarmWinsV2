alert("Credits: Learningfarmhaker on github");
function Autofarm() {
  tf.gameObj.goGame();
  setTimeout(function() {
    document.getElementById("oppCheckbox").checked = false;
  }, 500);
  setTimeout(function() {
    window.volleyGame.optSelected(1);
  }, 3000);
  setTimeout(function() {
    window.volleyGame.leftScore = 5;
  }, 4500);
  setTimeout(function() {
    window.volleyGame.gameOver();
  }, 6000);
}

while (true) {
  Autofarm();
  // Add a delay to avoid overloading the server
  await new Promise(resolve => setTimeout(resolve, 5000));
}
